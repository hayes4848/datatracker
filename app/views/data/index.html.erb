<h1>Listing data</h1>

<table>
  <tr>
    <th>Specks</th>
    <th>Light leaks</th>
    <th>Holes</th>
    <th>Boron cracks</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @data.each do |datum| %>
  <tr>
    <td><%= datum.specks %></td>
    <td><%= datum.light_leaks %></td>
    <td><%= datum.holes %></td>
    <td><%= datum.boron_cracks %></td>
    <td><%= link_to 'Show', datum %></td>
    <td><%= link_to 'Edit', edit_datum_path(datum) %></td>
    <td><%= link_to 'Destroy', datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

